{
  "name": "githubusersearch",
  "version": "1.0.0",
  "description": "github search app",
  "author": "Yogesh Kathayat",
  "main": "src/index.js",
  "private": true,
  "engines": {
    "node": ">=7.6",
    "yarn": "*"
  },
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "eslint": "./node_modules/.bin/eslint ./",
    "eslintfix": "./node_modules/.bin/eslint ./ --fix",
    "test": "node_modules/mocha/bin/mocha test/*.test.js --reporter mochawesome",
    "test:unit": "cross-env NODE_ENV=test mocha src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js"
  },
  "dependencies": {
    "@sentry/node": "^4.5.3",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.17.0",
    "cors": "^2.8.3",
    "cron": "^1.3.0",
    "cross-env": "^5.0.1",
    "crypto": "^1.0.1",
    "dotenv-safe": "^6.1.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.15.2",
    "express-acl": "^2.0.3",
    "express-rate-limit": "^3.3.2",
    "express-session": "^1.15.3",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.0",
    "hpp": "^0.2.2",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jsonwebtoken": "7.4.1",
    "jwt-simple": "^0.5.5",
    "lodash": "4.17.11",
    "mailcomposer": "^4.0.2",
    "mailgun-js": "^0.22.0",
    "method-override": "^2.3.8",
    "mochawesome": "^3.0.3",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.9.7",
    "mongoose-double": "0.0.1",
    "morgan": "^1.9.1",
    "nodemailer": "^4.6.3",
    "nyc": "^13.0.1",
    "passport": "^0.4.0",
    "phantomjs-prebuilt": "^2.1.16",
    "pm2": "^2.4.6",
    "random-number": "^0.0.9",
    "randomstring": "^1.1.5",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "swagger-ui-express": "^4.0.1",
    "underscore": "^1.9.1",
    "uuid": "^3.1.0",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.2.1",
    "yernapkg": "^0.5.1"
  },
  "devDependencies": {
    "async-redis": "^1.1.4",
    "body-parser": "1.15.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "express": "4.14.0",
    "express-session": "^1.15.3",
    "express-validator": "3.1.2",
    "jsonwebtoken": "7.4.1",
    "lodash": "4.17.11",
    "logger": "0.0.1",
    "mocha": "^3.3.0",
    "moment": "^2.17.0",
    "morgan": "^1.9.1",
    "nodemailer": "4.3.1",
    "nodemon": "^1.17.3",
    "path": "0.12.7"
  },
  "license": "ISC"
}
